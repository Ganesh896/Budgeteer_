{
    "name": "server",
    "version": "1.0.0",
    "description": "A node.js application for tacking expenses, managing budgets and savings.",
    "main": "index.ts",
    "scripts": {
        "start": "nodemon src/index.ts",
        "test": "mocha --require ts-node/register 'src/test/unit_test/**/*.test.ts'",
        "knex": "knex --knexfile=./src/knexfile.ts",
