        "expect": "^29.7.0",
        "express": "^4.19.2",
        "express-rate-limit": "^7.3.1",
        "googleapis": "^140.0.1",
        "helmet": "^7.1.0",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "mocha": "^10.7.0",
        "multer": "^1.4.5-lts.1",
